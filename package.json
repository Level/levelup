  "dependencies": {
    "deferred-leveldown": "~5.0.0",
    "level-errors": "~2.0.0",
    "level-iterator-stream": "~4.0.0",
    "xtend": "~4.0.0"
  },
  "devDependencies": {
    "after": "^0.8.2",
    "airtap": "^2.0.0",
    "async-each": "^1.0.3",
    "bl": "^3.0.0",
    "browserify": "^16.0.0",
    "bustermove": "^1.0.0",
    "concat-stream": "^2.0.0",
    "coveralls": "^3.0.2",
    "delayed": "^1.0.1",
    "dependency-check": "^3.3.0",
    "encoding-down": "^6.0.0",
    "hallmark": "^0.1.0",
    "level-community": "^3.0.0",
    "memdown": "^4.0.0",
    "nyc": "^14.0.0",
    "pinkie": "^2.0.4",
    "referee": "^1.2.0",
    "run-parallel": "^1.1.9",
