{
    "name"            : "levelup"
  , "description"     : "Fast & simple storage - a Node.js-style LevelDB wrapper"
  , "authors": [
        "Rod Vagg @rvagg <rod@vagg.org> (https://github.com/rvagg)"
    ]
  , "keywords"        : [
        "leveldb"
      , "levelup"
      , "stream"
      , "database"
      , "db"
      , "database"
      , "store"
      , "storage"
      , "json"
    ]
  , "version"         : "0.3.1"
  , "main"            : "lib/levelup.js"
  , "dependencies"    : {
        "errno"         : "~0.0.3"
      , "bindings"      : "~1.0.0"
      , "concat-stream" : "~0.0.9"
      , "bufferstream"  : "~0.5.1"
    }
  , "devDependencies" : {
        "buster"        : "*"
      , "rimraf"        : "*"
      , "async"         : "*"
      , "fstream"       : "*"
      , "tar"           : "*"
      , "mkfiletree"    : "*"
      , "readfiletree"  : "*"
      , "slow-stream"   : "*"
      , "delayed"       : "*"
      , "boganipsum"    : "*"
    }
  , "repository": {
        "type": "git"
      , "url": "https://github.com/rvagg/node-levelup.git"
    }
  , "scripts"         : {
        "test"            : "./node_modules/.bin/buster-test"
      , "install"         : "node-gyp rebuild"
      , "functionaltest"  : "node ./test/functional/fstream-test.js"
    }
  , "license"         : "MIT"
  , "gypfile"         : true
}
